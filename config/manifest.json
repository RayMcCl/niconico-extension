{
    "name": "NicoNico Extension - Development Build",
    "description": "Pithy description (132 characters or less, no HTML)",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {
        "128": "img/icon_128.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*",
                "http://www.youtube.com/*",
                "https://www.twitch.tv/*",
                "http://www.twitch.tv/*"
            ],
            "js": ["content_start.js"],
            "run_at": "document_end",
            "all_frames": true
        }
    ]
}